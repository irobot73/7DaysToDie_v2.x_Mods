<items>

<!-- XPath used to grab all ITEMS: //item/effect_group/triggered_effect[@action='GiveExp']/../../@name
        Everything defaults to 10% of vanilla values -->

<!-- Set EXP gain from reading the Duke's note -->
    <!-- Default EXP rate is 100 -->
	<set xpath="//item[@name='noteDuke01']/effect_group/triggered_effect[@action='GiveExp']/@exp">10</set>

<!-- Use the following to set ALL skill magazines -->
    <!-- Default EXP rate is 50 -->
	<set xpath="//item[contains(@name,'SkillMagazine')]/effect_group/triggered_effect[@action='GiveExp']/@exp">5</set>
  
<!-- Use the following to set ALL books -->
    <!-- Default EXP rate is 50 -->
	<set xpath="//item[starts-with(@name,'book')]/effect_group/triggered_effect[@action='GiveExp']/@exp">5</set>

<!-- Use the following to set ALL skill schematic -->
    <!-- Default EXP rate is 50 -->
	<set xpath="//item[contains(@name,'Schematic')]/effect_group/triggered_effect[@action='GiveExp']/@exp">5</set>

