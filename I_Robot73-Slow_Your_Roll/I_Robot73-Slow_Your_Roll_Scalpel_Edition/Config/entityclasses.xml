<?xml version="1.0" encoding="utf-8"?>

<entity_classes>

<!-- PlayerExpGain -->

    <!-- Set the EXP players will earn via TAGs
       or set an overall default by keeping ONE 'PlayerExpGain' and removing any/all TAGS)
     NOTE:  Overlap may cause LOSS of EXP (IE:  Make it generic and change EXP in other areas = stacking effect)

       EX: VANILLA EXP gains: Bldg block -> wood (96) -> cobble (120) -> concrete (240) -> steel (360)
                       '1.0': Bldg block -> wood (32) -> cobble (40)  -> concrete (80)  -> steel (120)  
                       '0.0': Bldg block -> wood (16) -> cobble (20)  -> concrete (40)  -> steel (60)
                      '-.75': Bldg block -> wood (4)  -> cobble (5)   -> concrete (10)  -> steel (15) 

     PlayerExpGain, Tags: Crafting, Harvesting, Kill, Looting, Other, Party, Quest, Selling, Upgrading -->
    <insertBefore xpath="//entity_class[@name='playerMale']/effect_group/passive_effect[@name='PlayerExpGain' and (@tags='Upgrading')]">

        <passive_effect name="PlayerExpGain" operation="perc_add" value="-.95" tags="Harvesting,Looting"/>
        <passive_effect name="PlayerExpGain" operation="perc_add" value="-.65" tags="Selling"/>

    </insertBefore>

   <!-- Set the EXP players will earn via TAGs or remove the TAG -->
   <!-- default value='5' -->
	<set xpath="//entity_class[@name='playerMale']/effect_group/passive_effect[@name='PlayerExpGain' and (@tags='Upgrading')]/@value">-.75</set>

   <!-- Remove the TAGS entirely (one entry to rule them all) -->
    <!-- <removeattribute xpath="//entity_class[@name='playerMale']/effect_group/passive_effect[@name='PlayerExpGain' and (@tags='Upgrading')]/@tags" /> -->
   <!-- Append other TAGS to the existing vanilla entry (don't utilize if adding individual TAGS [SEE: 'insertBefore'])
        NOTE:  WATCH THOSE COMMAS -->
    <append xpath="//entity_class[@name='playerMale']/effect_group/passive_effect[@name='PlayerExpGain' and (@tags='Upgrading')]/@tags">,Crafting</append>

<!-- ************************* -->

